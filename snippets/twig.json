  "section-slider (OS2)": {
    "prefix": ["section-slider", "ts-section-slider"],
    "body": [
      "<div data-toggle=\"slider\"",
      "     data-navigation=\"{{ SETTING.DISPLAY_NAVIGATION ? 1 : 0 }}\"",
      "     data-thumbnail=\"{{ SETTING.DISPLAY_THUMBNAIL ? 1 : 0 }}\"",
      "     {% if SETTING.PAGINATION_TYPE %}data-pagination-type=\"{{ SETTING.PAGINATION_TYPE }}\"{% endif %}",
      "     data-loop=\"{{ SETTING.LOOP ? 1 : 0 }}\"",
      "     data-autoplay=\"{{ SETTING.AUTOPLAY }}\"",
      "     data-effect=\"{{ SETTING.EFFECT }}\">",
      "  <div class=\"swiper\">",
      "    <div class=\"swiper-wrapper\">",
      "      {% for IMAGE in SETTING.IMAGES %}",
      "        <a class=\"swiper-slide\" href=\"{{ IMAGE.URL.URL }}\"",
      "           {% if IMAGE.URL.URL_TARGET %}target=\"{{ IMAGE.URL.URL_TARGET }}\"{% endif %}",
      "           {% if IMAGE.URL.URL_REL %}rel=\"{{ IMAGE.URL.URL_REL }}\"{% endif %}>",
      "          <picture>",
      "            {% if IMAGE.IMG2.PATH %}",
      "              <source media=\"(max-width: 768px)\" srcset=\"{{ IMAGE.IMG2.PATH }}\">",
      "            {% endif %}",
      "            <img src=\"{{ IMAGE.IMG1.PATH }}\" width=\"{{ IMAGE.IMG1.WIDTH }}\" height=\"{{ IMAGE.IMG1.HEIGHT }}\" alt=\"{{ IMAGE.TITLE }}\">",
      "          </picture>",
      "          {% if IMAGE.BTNTEXT %}<span>{{ IMAGE.BTNTEXT }}</span>{% endif %}",
      "          {% if IMAGE.CONTENT %}<div>{{ IMAGE.CONTENT|raw }}</div>{% endif %}",
      "        </a>",
      "      {% endfor %}",
      "    </div>",
      "  </div>",
      "</div>"
    ]
  },
  "section-banner (OS2)": {
    "prefix": ["section-banner", "ts-section-banner"],
    "body": [
      "<div class=\"section-banner\">",
      "  {% if SETTING.TITLE %}<h2>{{ SETTING.TITLE }}</h2>{% endif %}",
      "  {% if SETTING.SUBTITLE %}<p>{{ SETTING.SUBTITLE }}</p>{% endif %}",
      "  <div class=\"banner-list\">",
      "    {% for IMAGE in SETTING.IMAGES %}",
      "      <a href=\"{{ IMAGE.URL.URL }}\"",
      "         class=\"banner-item\"",
      "         {% if IMAGE.URL.URL_TARGET %}target=\"{{ IMAGE.URL.URL_TARGET }}\"{% endif %}",
      "         {% if IMAGE.URL.URL_REL %}rel=\"{{ IMAGE.URL.URL_REL }}\"{% endif %}>",
      "        <picture>",
      "          {% if IMAGE.IMG2.PATH %}",
      "            <source media=\"(max-width: 768px)\" srcset=\"{{ IMAGE.IMG2.PATH }}\">",
      "          {% endif %}",
      "          <img src=\"{{ IMAGE.IMG1.PATH }}\" width=\"{{ IMAGE.IMG1.WIDTH }}\" height=\"{{ IMAGE.IMG1.HEIGHT }}\" alt=\"{{ IMAGE.TITLE }}\">",
      "        </picture>",
      "        <div class=\"banner-meta\">",
      "          {% if IMAGE.TITLE %}<strong>{{ IMAGE.TITLE }}</strong>{% endif %}",
      "          {% if IMAGE.SUBTITLE %}<span>{{ IMAGE.SUBTITLE }}</span>{% endif %}",
      "          {% if IMAGE.BTNTEXT %}<em>{{ IMAGE.BTNTEXT }}</em>{% endif %}",
      "        </div>",
      "        {% if IMAGE.CONTENT %}<div class=\"banner-content\">{{ IMAGE.CONTENT|raw }}</div>{% endif %}",
      "      </a>",
      "    {% endfor %}",
      "  </div>",
      "</div>"
    ]
  },
  "toggle: popup example": {
    "prefix": ["toggle-popup", "data-popup"],
    "body": [
      "<span data-url=\"${1:/srv/service/content-v5/fixed-page-content/-5}\"",
      "      data-width=\"${2:1000}\"",
      "      data-property=\"${3:CONTENT}\"",
      "      data-toggle=\"popup\">${0}</span>"
    ]
  },
  "toggle: product-loader example": {
    "prefix": ["toggle-product-loader", "data-product-loader"],
    "body": [
      "<div id=\"catalog-{{ BLOCK.ID }}\">",
      "  <div data-toggle=\"product\"></div>",
      "</div>",
      "<input type=\"hidden\"",
      "       data-id=\"{{ BLOCK.ID }}\"",
      "       data-target=\"#catalog-{{ BLOCK.ID }}\"",
      "       data-item=\"[data-toggle='product']\"",
      "       data-toggle=\"product-loader\">"
    ]
  },
  "toggle: showcase example": {
    "prefix": ["toggle-showcase", "data-showcase"],
    "body": [
      "<div data-toggle=\"showcase\">",
      "  {% for TAB in SETTING.TABS %}",
      "  <button type=\"button\" class=\"{% if TAB.INDEX == FIRST_TAB.INDEX %}active{% endif %}\"",
      "          {% if TAB.INDEX != FIRST_TAB.INDEX %} data-url=\"{{ TAB.SHOWCASE.API }}\" {% endif %}",
      "          data-toggle=\"showcase-button\"",
      "          data-showcase-tab=\"#showcase-{{ BLOCK.ID }}{{ TAB.INDEX }}\"",
      "          {% if not TAB.TITLE %} aria-label=\"showcase-button\" {% endif %}>{{ TAB.TITLE }}</button>",
      "  {% endfor %}",
      "</div>",
      "",
      "{% for key, TAB in SETTING.TABS %}",
      "  {% if TAB.INDEX != FIRST_TAB.INDEX %}",
      "  <div id=\"showcase-{{ BLOCK.ID }}{{ TAB.INDEX }}\">",
      "    <div>",
      "      <template data-showcase-products></template>",
      "    </div>",
      "  </div>",
      "  {% endif %}",
      "{% endfor %}"
    ]
  },
  "toggle: light-gallery example": {
    "prefix": ["toggle-light-gallery", "data-light-gallery"],
    "body": [
      "<div data-toggle=\"light-gallery\">",
      "  <a href=\"${1:{{ X.URL }}}\">",
      "    <img src=\"${2:{{ X.PATH }}}\" width=\"${3:100}\" height=\"${4:100}\">",
      "  </a>",
      "</div>"
    ]
  },
  "toggle: zoom example": {
    "prefix": ["toggle-zoom", "data-zoom"],
    "body": [
      "<a class=\"aspect-product\" href=\"{{ IMAGE.BIG }}\" data-id=\"{{ IMAGE.VARIANT_TYPE_ID }}\" data-toggle=\"product-image\">",
      "  <picture>",
      "    <img data-toggle=\"zoom\" data-scroll=\"${1:1}\" data-image=\"${2:{{ IMAGE.BIG }}}\">",
      "  </picture>",
      "</a>"
    ]
  },
